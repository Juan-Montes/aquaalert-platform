          "datasource": {
            "type": "postgres",
            "uid": "timescaledb"
          },
          "rawSql": "SELECT time,\n  CASE alert_level\n    WHEN 'NORMAL'   THEN 0\n    WHEN 'WATCH'    THEN 1\n    WHEN 'WARNING'  THEN 2\n    WHEN 'CRITICAL' THEN 3\n    ELSE 0\n  END as \"Alerta\"\nFROM sensor_readings\nWHERE device_eui = ''\nORDER BY time DESC LIMIT 1",
